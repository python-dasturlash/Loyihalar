def Aralash():
    a = 1;  b = 10
    t_javoblar_soni = 0
    massiv = []
    
    while a <= b:
        n = randint(1,43)
        index = [0,1,2]
        variant = ['a', 'b', 'c']
        
        if n not in massiv:
            massiv.append(n)
            if n == 1:
                print(f"{a}) BAA poytaxti qayer?")
                javob = ["Abu Dabi", "Amman", "Bog'dod"]
                
