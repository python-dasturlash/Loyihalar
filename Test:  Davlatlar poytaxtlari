                    print("To'g'ri\n")
                    t_javoblar_soni += 1
                else:
                    print("Noto'g'ri\n")
            
            
            a += 1
        
    
    return t_javoblar_soni
        
        

while True:
    print("\nQitani tanlang:"
          "\n1) Osiyo"
          "\n2) Yevropa"
          "\n3) Amerika"
          "\n4) Afrika"
          "\n5) Aralash")
    
    n = input("n==> ")
    
    if n == '1':
        print("Siz Osiyo qitasini tanladingiz.\n")
        n = Osiyo()
        print(f"Siz 10 ta savoldan {n} tasiga to'g'ri javob berdingiz.")
        print(f"Natijangiz:  {n*10}%")
        print("\nTestni davom ettirishni hohlaysizmi?"
              "\n1) Ha"
              "\n2) Yo'q")
        n = input("==> ")
        if n == "Ha" or n == "ha" or n == "1":
            continue
        else:
            print("Good luck")
            break
    
    
    elif n == '2':
        print("Siz Yevropa qitasini tanladingiz.\n")
        n = Yevropa()
        print(f"Siz 10 ta savoldan {n} tasiga to'g'ri javob berdingiz.")
        print(f"Natijangiz:  {n*10}%")
        print("\nTestni davom ettirishni hohlaysizmi?"
              "\n1) Ha"
              "\n2) Yo'q")
        n = input("==> ")
