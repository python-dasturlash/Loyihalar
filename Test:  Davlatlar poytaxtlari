                            togri_javob = variant[0]
                        variant.pop(0)
                        index.remove(n)
                    
                    
                n = input("Javob: ")     
                
                if n == togri_javob or n == togri_javob.upper():
                    print("To'g'ri\n")
                    t_javoblar_soni += 1
                else:
                    print("Noto'g'ri\n")
            
            
            
            elif n == 158:
                print(f"{a}) Tunis poytaxti qayer?")
                javob = ["Tunis", "Praya", "Luanda"]
                
                while True:              
                    n = randint(0,2)
                    if len(index) == 0:
                        break
                    if n in index:
                        print(f"{variant[0]}) {javob[n]}",end='   ')
                        if n == 0:
                            togri_javob = variant[0]
                        variant.pop(0)
                        index.remove(n)
                    
                    
                n = input("Javob: ")     
                
                if n == togri_javob or n == togri_javob.upper():
                    print("To'g'ri\n")
                    t_javoblar_soni += 1
                else:
                    print("Noto'g'ri\n")
            
            
            
            elif n == 159:
                print(f"{a}) Burkina Faso poytaxti qayer?")
                javob = ["Uagadugu", "Lome", "Fritaun"]
                
