
        
from random import randint

def Osiyo():
    a = 1;  b = 10
    t_javoblar_soni = 0
    massiv = []
    
    while a <= b:
        n = randint(1,43)
        index = [0,1,2]
        variant = ['a', 'b', 'c']
        
        if n not in massiv:              # Test davomida bitta savol qayta takrorlanmasligi uchun
            massiv.append(n)
            if n == 1:
                print(f"{a}) BAA poytaxti qayer?")
                javob = ["Abu Dabi", "Amman", "Bog'dod"]
                
                while True:              # Javoblarni har safar joyini almashtirish
                    n = randint(0,2)
                    if len(index) == 0:
                        break
                    if n in index:
                        print(f"{variant[0]}) {javob[n]}",end='   ')
                        if n == 0:
                            togri_javob = variant[0]
                        variant.pop(0)
                        index.remove(n)
                    
                    
                n = input("Javob: ")     # To'g'ri javobni tanlash
                
                if n == togri_javob or n == togri_javob.upper():
                    print("To'g'ri\n")
                    t_javoblar_soni += 1
                else:
                    print("Noto'g'ri\n")
                    
                    
                    

            elif n == 2:
                print(f"{a}) Iordaniya poytaxti qayer?")
                javob = ["Amman", "Bayrut", "Boku"]
                
                while True:
                    n = randint(0,2)
                    if len(index) == 0:
                        break
                    if n in index:
                        print(f"{variant[0]}) {javob[n]}",end='   ')
                        if n == 0:
                            togri_javob = variant[0]
                        variant.pop(0)
                        index.remove(n)
                
                
                n = input("Javob: ")
                
                if n == togri_javob or n == togri_javob.upper():
                    print("To'g'ri\n")
                    t_javoblar_soni += 1
                else:
                    print("Noto'g'ri\n")
                
                    
            
            
            elif n == 3:
